{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "> 0.25%, not dead",
          "not IE 11",
          "maintained node versions"
        ]
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": 3,
        "helpers": true,
        "regenerator": true
      }
    ]
  ],
  "sourceMaps": true
}
